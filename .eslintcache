chat-clone-yt/src/features/appSlice.js",[],"/Users/jonathanperry/WebApps/snapchat-clone-yt/src/WebcamCapture.js",["61","62"],"import React, { useCallback, useRef, useState } from \"react\";\nimport Webcam from \"react-webcam\";\nimport RadioButtonUncheckedIcon from \"@material-ui/icons/RadioButtonUnchecked\";\nimport { useDispatch } from \"react-redux\";\nimport { setCameraImage } from \"./features/cameraSlice\";\nimport { useHistory } from \"react-router-dom\";\nimport \"./WebcamCapture.css\";\n\nconst videoConstraints = {\n  width: 250,\n  height: 400,\n  facingMode: \"user\",\n};\n\nfunction WebcamCapture() {\n  const webcamRef = useRef(null);\n  const dispatch = useDispatch();\n  const history = useHistory();\n\n  const [image, setImage] = useState(null);\n\n  const capture = useCallback(() => {\n    const imageSrc = webcamRef.current.getScreenshot();\n    dispatch(setCameraImage(imageSrc));\n    history.push(\"/preview\");\n  }, [webcamRef]);\n\n  return (\n    <div className=\"webcamCapture\">\n      <Webcam\n        audio={false}\n        height={videoConstraints.height}\n        ref={webcamRef}\n        width={videoConstraints.width}\n        videConstraints={videoConstraints}\n      />\n\n      <RadioButtonUncheckedIcon\n        className=\"webcamCapture__button\"\n        onClick={capture}\n        fontSize=\"large\"\n      />\n      <img src={image} alt=\"\" />\n    </div>\n  );\n}\n\nexport default WebcamCapture;\n","/Users/jonathanperry/WebApps/snapchat-clone-yt/src/features/cameraSlice.js",[],"/Users/jonathanperry/WebApps/snapchat-clone-yt/src/Preview.js",[],"/Users/jonathanperry/WebApps/snapchat-clone-yt/src/firebase.js",[],"/Users/jonathanperry/WebApps/snapchat-clone-yt/src/Chats.js",[],"/Users/jonathanperry/WebApps/snapchat-clone-yt/src/Chat.js",["63"],"import React from \"react\";\nimport { Avatar } from \"@material-ui/core\";\nimport \"./Chat.css\";\nimport { StopRounded } from \"@material-ui/icons\";\nimport ReactTimeago from \"react-timeago\";\n