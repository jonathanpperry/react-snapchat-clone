 colors={[\n            [\"#004777\", 0.33],\n            [\"#F7B801\", 0.33],\n            [\"#A30000\", 0.33],\n          ]}\n        >\n          {({ remainingTime }) => {\n            if (remainingTime === 0) {\n              exit();\n            }\n            return remainingTime;\n          }}\n        </CountdownCircleTimer>\n      </div>\n    </div>\n  );\n}\n\nexport default ChatView;\n","/Users/jonathanperry/WebApps/snapchat-clone-yt/src/Login.js",[],{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":32,"column":6,"nodeType":"71","endLine":32,"endColumn":8,"suggestions":"72"},{"ruleId":"73","severity":1,"message":"74","line":20,"column":17,"nodeType":"75","messageId":"76","endLine":20,"endColumn":25},{"ruleId":"69","severity":1,"message":"77","line":26,"column":6,"nodeType":"71","endLine":26,"endColumn":17,"suggestions":"78"},{"ruleId":"79","severity":1,"message":"80","line":32,"column":34,"nodeType":"81","endLine":32,"endColumn":50},{"ruleId":"69","severity":1,"message":"82","line":16,"column":6,"nodeType":"71","endLine":16,"endColumn":8,"suggestions":"83"},"no-native-reassign",["84"],"no-negated-in-lhs",["85"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.","ArrayExpression",["86"],"no-unused-vars","'setImage' is assigned a value but never used.","Identifier","unusedVar","React Hook useCallback has missing dependencies: 'dispatch' and 'history'. Either include them or remove the dependency array.",["87"],"react/jsx-no-duplicate-props","No duplicate props allowed","JSXAttribute","React Hook useEffect has missing dependencies: 'exit' and 'selectedImage'. Either include them or remove the dependency array.",["88"],"no-global-assign","no-unsafe-negation",{"desc":"89","fix":"90"},{"desc":"91","fix":"92"},{"desc":"93","fix":"94"},"Update the dependencies array to be: [dispatch]",{"range":"95","text":"9